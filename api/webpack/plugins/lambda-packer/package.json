{
  "name": "lambda-packer",
  "version": "0.0.0",
  "description": "Webpack plugin for building AWS Lambda Packages",
  "author": "Garrett Culos",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "watch": "webpack --config webpack.watch.js",
    "build.prod": "webpack --config webpack.prod.js",
    "clean": "rimraf node_modules dist && npm install",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --coverage",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"lambda-packer/**/*{.ts,.js,.json}\"",
    "prettier:fix": "prettier --config ./.prettierrc --staged --write \"lambda-packer/**/*\"",
    "prettier:quick": "pretty-quick --stage"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/archiver": "^2.1.3",
    "@types/cpx": "^1.5.1",
    "@types/fs-extra": "^5.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.10",
    "@types/make-dir": "^2.1.0",
    "@types/mv": "^2.1.0",
    "@types/node": "^11.13.0",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.27",
    "archiver": "^3.1.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-latest-node": "^3.1.1",
    "cpx": "^1.5.0",
    "fs-extra": "^7.0.1",
    "jest": "^26.4.2",
    "make-dir": "^3.0.0",
    "mv": "^2.1.1",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.4.3"
  }
}
