{
  "name": "h3-monolith",
  "version": "0.0.1",
  "description": "TS/JS Monolith",
  "author": "Garrett Culos",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules dist && npm install",
    "lint": "eslint **/*.ts",
    "test": "jest --coverage",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"**/*{.ts,.js,.json}\"",
    "prettier:fix": "prettier --config ./.prettierrc --staged --write \"**/*\"",
    "prettier:quick": "pretty-quick --stage",
    "graphqlSamEg.dev": "concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run graphqlSamEg.build.watch\" \"nodemon ./_graphql_sam_example/dist/src/graphql/express/express.js --watch\"",
    "graphqlSamEg.seed.dynamodb": "ts-node ./_graphql_sam_example/scripts/dynamodb-startup-script.ts",
    "graphqlSamEg.build.serverless": "webpack --config ./_graphql_sam_examplewebpack/webpack.serverless.js",
    "graphqlSamEg.build.watch": "webpack --config ./_graphql_sam_example/webpack/webpack.graphql.watch.js",
    "graphqlSamEg.build.docker": "webpack --config ./_graphql_sam_example/webpack/webpack.graphql.docker.js && nodemon ./_graphql_sam_example/dist/src/graphql/express/express.js",
    "graphqlSamEg.awsSamPack": "sam package --template-file ./_graphql_sam_example/graphql-sam-deploy/template.json --output-template-file ./_graphql_sam_example/graphql-sam-deploy/sam-template.yml --s3-bucket store-portal-serverless-api",
    "graphqlSamEg.awsSamDeploy": "sam deploy --template-file ./_graphql_sam_example/graphql-sam-deploy/sam-template.yml --stack-name store-portal-serverless-api --capabilities CAPABILITY_NAMED_IAM",
    "graphqlSamEg.serverless.build.pack.deploy": "npm run graphqlSamEg.build.serverless && npm run graphqlSamEg.awsSamPack && npm run graphqlSamEg.awsSamDeploy"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/async": "^3.2.1",
    "@types/aws-lambda": "^8.10.24",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/express": "^4.11.1",
    "@types/graphql": "^14.2.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lz-string": "^1.3.33",
    "@types/node": "^11.15.12",
    "@types/request": "^2.48.4",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-server": "^2.4.8",
    "apollo-server-lambda": "^2.4.8",
    "apollo-server-testing": "^2.11.0",
    "async": "^3.2.0",
    "aws-sdk": "^2.608.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-latest-node": "^3.1.1",
    "bcrypt-nodejs": "^0.0.3",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "google-auth-library": "^5.9.1",
    "graphql": "^14.2.1",
    "graphql-type-json": "^0.3.1",
    "husky": "^1.3.1",
    "jest": "^25.5.4",
    "jsonwebtoken": "^8.3.0",
    "lz-string": "^1.4.4",
    "moment": "^2.24.0",
    "node-ts": "^5.0.4",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "sam-webpack-plugin": "^0.1.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.9.0",
    "typescript": "^3.8.3",
    "uuid": "^3.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:quick && npm run test && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/highwaythree/h3-monolith-base"
  },
  "bugs": {
    "url": "https://bitbucket.org/highwaythree/h3-monolith-base"
  },
  "homepage": "https://bitbucket.org/highwaythree/h3-monolith-base"
}
